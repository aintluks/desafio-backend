<div class="modal fade" id="fileModal" tabindex="-1" aria-labelledby="fileModal" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="fileModal"><%= t('.title') %></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <%= form_for @file_upload do |form| %>
          <div class="input-group mb-3">
            <%= form.file_field :csv_file, class: "form-control", accept: ".csv" %>
            <%= form.label t('.upload'), class: "input-group-text" %>
          </div>
          <div class="d-flex justify-content-end">
            <%= form.submit t('.submit'), class: "btn btn-primary" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>